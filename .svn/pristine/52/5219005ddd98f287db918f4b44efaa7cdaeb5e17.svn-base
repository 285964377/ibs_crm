.layui-treeSelect .ztree li span.button.switch{
	position: relative;
	top:-7px;
}
/* 颜色 */
.fwt{
	font-weight:bold;
}
.blue{
	color:#0066ff;
}
.yellow{
	color:#ffbd60;
}
.head_top_table{
	background:#eff3f5;
	padding:10px 10px;
	overflow:hidden;
	border:1px solid #c6d1d6;
	line-height: 20px;
}
.head_top_table li{
	float:left;
	margin-right:30px;
}
.my_style_table{
	margin-top:16px;
}
.my_style_table_body{
	overflow:hidden;
}
.my_style_table_body li{
	overflow:hidden;
}
.my_style_table_body li>div{
	font-size:12px;
	border:1px solid #c6d1d6;
	height:28px;
	line-height: 28px;
	overflow: hidden;
	text-overflow:ellipsis;
	white-space: nowrap;
	text-align:center;
}
.my_table_new li{
	float:left;
	width:10%;
}
.tab_list li{
	padding:6px 8px;
	background:#dfdfdf;
	float:left;
	margin-right:7px;
	font-size:12px;
}
.my_sj_list {
	margin-top:20px;
	font-size:12px;
}
.my_sj_list div{
	padding:20px;
	border:1px solid #c6d1d6;
	background:#eff3f5;
}